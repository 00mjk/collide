<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder">
	<ui:with field="res" type="collide.gwtc.resources.GwtcResources" />
	<div ui:field="headerContainer" class="{res.panelHeaderCss.headerContainer}">
		<div class="{res.panelHeaderCss.topHeader}">
			<div ui:field="icons" class="{res.panelHeaderCss.icons}">
				<div ui:field="reload" class="{res.panelHeaderCss.reloadIcon}" title="Reload test page" />
				<div ui:field="status" class="{res.panelHeaderCss.gear}" title="Recompile test page"/>
			</div>
			<h2 ui:field="header" class="{res.panelHeaderCss.header}">
			</h2>
			<div ui:field="controls" class="{res.panelHeaderCss.controls}">
			  <div>
  			  <div ui:field="close" class="{res.panelHeaderCss.close}"></div>
			  </div>
			</div>
		</div>
		<div class="{res.panelHeaderCss.bottomHeader}" style="display:none">
      <div>Actions</div>
      <div>All Source</div>
      <div ui:field="generated"> </div>
		</div>
	</div>
</ui:UiBinder>